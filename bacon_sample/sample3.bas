9220  IF CE(I)=1 THEN GOTO 9260
9230  IF RND(1)*15>1 THEN GOTO 9420
9240  CX(I)=RND(1)*27:CY(I)=3:CE(I)=1
9250  GOTO 9330
9260  CY(I)=CY(I)+1:IF CY(I)<19 THEN 9330
9270  FOR X=0 TO 5:VPOKE &H1800+(18)*32+CX(I)+X,&H20:NEXT X
9280  FOR X=0 TO 5:VPOKE &H1800+(19)*32+CX(I)+X,&H20:NEXT X
9290  FOR X=0 TO 5:VPOKE &H1800+(20)*32+CX(I)+X,&H20:NEXT X
9300  FOR X=0 TO 5:VPOKE &H1800+(21)*32+CX(I)+X,&H20:NEXT X
9310  CX(I)=0:CY(I)=0:CE(I)=0
9320  GOTO 9420
9330  J=0
9340  FOR X=0 TO 5:VPOKE &H1800+(CY(I)-1)*32+CX(I)+X,&H20:NEXT X
